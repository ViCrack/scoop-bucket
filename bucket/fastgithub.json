{
    "version": "2.1.3",
    "description": "github加速神器，解决github打不开、用户头像无法加载、releases无法上传下载、git-clone、git-pull、git-push失败等问题. ",
    "homepage": "https://github.com/dotnetcore/FastGithub",
    "license": "MIT",
    "depends": "sudo",
    "architecture": {
        "64bit": {
            "url": "https://github.com/dotnetcore/FastGithub/releases/download/2.1.3/fastgithub_win-x64.zip",
            "hash": "4f5f87fe75607276aa561991eeb618c2d049e7498f50d12453a9ae96f3952637"
        }
    },
    "extract_dir": "fastgithub_win-x64",
    "uninstaller": {
        "script": [
            "sudo taskkill /F /IM 'FastGithub.UI.exe' /FI 'status eq running' | Out-Null",
            "sudo \"$dir\\fastgithub.exe\" stop"
        ]
    },
    "bin": [
        "fastgithub.exe",
        "FastGithub.UI.exe"
    ],
    "shortcuts": [
        [
            "FastGithub.UI.exe",
            "FastGithub.UI"
        ]
    ],
    "persist": [
        "appsettings",
        "cacert",
        "logs",
        "appsettings.json"
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/dotnetcore/FastGithub/releases/download/$version/fastgithub_win-x64.zip"
            }
        }
    }
}
